package edureka;

public class throwExample {

	public static void avg()
	{
		try {
			throw new ArithmeticException("demo");
		}
		catch(ArithmeticException e)
		{
			System.out.println("caught");
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		avg();
	}

}
