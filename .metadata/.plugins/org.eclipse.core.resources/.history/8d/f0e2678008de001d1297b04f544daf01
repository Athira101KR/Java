package codeShef;

import java.util.Scanner;

public class consecitiveConsonents {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int a;
		String s;
		for(int i=0;i<n;i++)
		{
			a=sc.nextInt();
			s=sc.nextLine();
			if(s.equals(null))
			{
				s=sc.nextLine();
			}
			//System.out.println(s);
		}
		
	}

}
